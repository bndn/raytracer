{
    "version": "0.0.0",
    "description": "A semi-efficient raytracing back-end powered by caffeine and bad puns",
    "authors": [
        "Anders Munck Højsgaard <amuh@itu.dk>",
        "Andreas Bjørn Hassing Nielsen <abhn@itu.dk>",
        "Kasper Kronborg Isager <kasi@itu.dk>",
        "Kathrin Solveig Schmid Jonsson <ksoj@itu.dk>",
        "Markus Thomsen <matho@itu.dk>",
        "Mathias Grundtvig Andreasen <mgru@itu.dk>"
    ],
    "tags": [
        "raytracer",
        "back",
        "end",
        "fsharp"
    ],
    "projectUrl": "https://github.itu.dk/bndn/raytracer",
    "licenseUrl": "https://github.itu.dk/bndn/raytracer/blob/master/LICENSE.md",

    "dependencies": {
        "Color": "",
        "Light": "",
        "Material": "",
        "Point": "",
        "Ray": "",
        "Shape": "",
        "Texture": "",
        "Vector": "",
        "Transform": "",
        "FSharp.Dnx": {
            "type": "build",
            "version": "1.0.0-*"
        }
